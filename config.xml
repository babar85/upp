<?xml version='1.0' encoding='utf-8'?>
<widget id="com.upp.3mobile" version="11.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>UPP</name>
    <description>
        United Printing and Publishing.
    </description>
    <author email="info@upp.ae" href="https://www.upp.ae">
        United Printing and Publishing
    </author>
    <content src="index.html" />
    <allow-navigation href="*" />
    <allow-intent href="*" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <preference name="android-minSdkVersion" value="14" />
		 	  <icon src="img/UPP.png" density="ldpi" />
              <icon src="img/UPP.png" density="mdpi" />
              <icon src="img/UPP.png" density="hdpi" />
              <icon src="img/UPP.png" density="xhdpi" />
        <allow-intent href="market:*" />
    </platform>
	
	<platform name="android">
             
     </platform>
	
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="BackupWebStorage" value="none" />
    </platform>
    <preference name="DisallowOverscroll" value="true" />
    <plugin name="cordova-plugin-console" spec="~1.0.1" />
    <plugin name="cordova-plugin-statusbar" spec="~1.0.1" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.2" />
</widget>
